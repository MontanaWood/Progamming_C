#include <stdio.h>
#include <stdlib.h>
#include <cstring> // Добавляем заголовочный файл для использования функции strrchr

int main() {
    FILE *input_file = fopen("people.txt", "r");
    FILE *output_file = fopen("born_after_1980.txt", "w");

    if (input_file == NULL || output_file == NULL) {
        perror("Error opening file");
        exit(EXIT_FAILURE);
    }

    char line[100]; // Предполагаем, что строка не длиннее 100 символов

    while (fgets(line, sizeof(line), input_file)) {
        // Получаем последнее число (год) из строки
        int year;
        sscanf(strrchr(line, ' ') + 1, "%d", &year);

        // Проверяем, позднее ли год рождения 1980
        if (year > 1980) {
            fprintf(output_file, "%s", line);
        }
    }

    fclose(input_file);
    fclose(output_file);

    return 0;
}
